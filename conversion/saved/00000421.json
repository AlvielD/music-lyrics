            "singer": [
                "ANNA ASTI"
            ],
            "lines": [
                {
                    "line": "Давай забудем всё и убежим от всех,",
                    "time_index": [
                        10.239583333333334,
                        15.139583333333334
                    ],
                    "time_duration": 4.9
                },
                {
                    "line": "И пусть целый мир подождёт нас,",
                    "time_index": [
                        15.239583333333334,
                        17.63958333333333
                    ],
                    "time_duration": 2.399999999999997
                },
                {
                    "line": "Мы пропали до завтра.",
                    "time_index": [
                        17.739583333333332,
                        20.13958333333333
                    ],
                    "time_duration": 2.3999999999999986
                },
                {
                    "line": "Только в твоих я руках,",
                    "time_index": [
                        20.239583333333332,
                        22.13958333333333
                    ],
                    "time_duration": 1.8999999999999986
                },
                {
                    "line": "Будто бы таю как снег,",
                    "time_index": [
                        22.239583333333332,
                        24.63958333333333
                    ],
                    "time_duration": 2.3999999999999986
                },
                {
                    "line": "Ты для меня моя крепость,",
                    "time_index": [
                        24.739583333333332,
                        27.13958333333333
                    ],
                    "time_duration": 2.3999999999999986
                },
                {
                    "line": "Моя вера и правда.",
                    "time_index": [
                        27.239583333333332,
                        29.38958333333333
                    ],
                    "time_duration": 2.1499999999999986
                },
                {
                    "line": "Я за тебя своё сердце,",
                    "time_index": [
                        29.489583333333332,
                        31.88958333333333
                    ],
                    "time_duration": 2.3999999999999986
                },
                {
                    "line": "Я свою жизнь, свою душу,",
                    "time_index": [
                        31.989583333333332,
                        34.389583333333334
                    ],
                    "time_duration": 2.400000000000002
                },
                {
                    "line": "Только один мне так нужен,",
                    "time_index": [
                        34.489583333333336,
                        35.639583333333334
                    ],
                    "time_duration": 1.1499999999999986
                },
                {
                    "line": "Мой самый лучший.",
                    "time_index": [
                        35.739583333333336,
                        39.139583333333334
                    ],
                    "time_duration": 3.3999999999999986
                },
                {
                    "line": "Снова в мурашках все плечи,",
                    "time_index": [
                        39.239583333333336,
                        41.389583333333334
                    ],
                    "time_duration": 2.1499999999999986
                },
                {
                    "line": "Просто целуй меня нежно,",
                    "time_index": [
                        41.489583333333336,
                        44.389583333333334
                    ],
                    "time_duration": 2.8999999999999986
                },
                {
                    "line": "Держи меня крепче, всё безупречно.",
                    "time_index": [
                        44.489583333333336,
